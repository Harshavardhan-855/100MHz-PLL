value="
.op
.options acct list
.temp 25

.include /usr/local/share/pdk/sky130A/libs.ref/sky130_fd_sc_hd/spice/sky130_fd_sc_hd.spice

.param vctrl=0.82 vdd=1.8 cload=250f
+ td=20n tr=0.1n tw=10n  tp=20n

.control
save all
save @m.xm1.msky130_fd_pr__nfet_01v8[cgd]
save @m.xm1.msky130_fd_pr__nfet_01v8[gm]

op
write inv_delay01.raw
set appendwrite

tran 0.001n 100n
meas tran tdelay01 TRIG v(out4) VAL=0.9 RISE=15 TARG v(out4) VAL=0.9 RISE=16
let fosc1= 1/tdelay01
print fosc1
save fosc1
remzerovec
write inv_delay01.raw


alterparam vctrl=0.83
reset
set appendwrite
tran 0.001n 100n
meas tran tdelay01 TRIG v(out4) VAL=0.9 RISE=15 TARG v(out4) VAL=0.9 RISE=16
let fosc1= 1/tdelay01
print fosc1
save fosc1
remzerovec 
write inv_delay01.raw

.endc
"
}
